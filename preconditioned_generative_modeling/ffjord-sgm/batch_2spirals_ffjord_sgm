#!/bin/bash
#SBATCH -c 4  # Number of Cores per Task
#SBATCH --mem=8192  # Requested Memory
#SBATCH -p cpu  # Partition
#SBATCH -t 24:00:00  # Job time limit
#SBATCH -o slurm-2spirals-ffjord-sgm.out  # %j = job ID

module load miniconda
conda activate generativetmula


python ffjord_sgm.py --data 2spirals --depth 1 --dims 64 
python ffjord_sgm.py --data 2spirals --depth 3 --dims 64 
python ffjord_sgm.py --data 2spirals --depth 5 --dims 64 
python ffjord_sgm.py --data 2spirals --depth 7 --dims 64 


python ffjord_sgm.py --data 2spirals --depth 1 --dims 32 
python ffjord_sgm.py --data 2spirals --depth 3 --dims 32 
python ffjord_sgm.py --data 2spirals --depth 5 --dims 32 
python ffjord_sgm.py --data 2spirals --depth 7 --dims 32 

python ffjord_sgm.py --data 2spirals --depth 1 --dims 16 
python ffjord_sgm.py --data 2spirals --depth 3 --dims 16 
python ffjord_sgm.py --data 2spirals --depth 5 --dims 16 
python ffjord_sgm.py --data 2spirals --depth 7 --dims 16 

python ffjord_sgm.py --data 2spirals --depth 1 --dims 8 
python ffjord_sgm.py --data 2spirals --depth 3 --dims 8 
python ffjord_sgm.py --data 2spirals --depth 5 --dims 8 
python ffjord_sgm.py --data 2spirals --depth 7 --dims 8 

python ffjord_sgm.py --data 2spirals --depth 1 --dims 4
python ffjord_sgm.py --data 2spirals --depth 3 --dims 4 
python ffjord_sgm.py --data 2spirals --depth 5 --dims 4 
python ffjord_sgm.py --data 2spirals --depth 7 --dims 4 





python ffjord_sgm.py --data 2spirals --depth 1 --dims 64 --rff True
python ffjord_sgm.py --data 2spirals --depth 3 --dims 64 --rff True
python ffjord_sgm.py --data 2spirals --depth 5 --dims 64 --rff True
python ffjord_sgm.py --data 2spirals --depth 7 --dims 64 --rff True


python ffjord_sgm.py --data 2spirals --depth 1 --dims 32 --rff True
python ffjord_sgm.py --data 2spirals --depth 3 --dims 32 --rff True
python ffjord_sgm.py --data 2spirals --depth 5 --dims 32 --rff True
python ffjord_sgm.py --data 2spirals --depth 7 --dims 32 --rff True

python ffjord_sgm.py --data 2spirals --depth 1 --dims 16 --rff True
python ffjord_sgm.py --data 2spirals --depth 3 --dims 16 --rff True
python ffjord_sgm.py --data 2spirals --depth 5 --dims 16 --rff True
python ffjord_sgm.py --data 2spirals --depth 7 --dims 16 --rff True

python ffjord_sgm.py --data 2spirals --depth 1 --dims 8 --rff True
python ffjord_sgm.py --data 2spirals --depth 3 --dims 8 --rff True
python ffjord_sgm.py --data 2spirals --depth 5 --dims 8 --rff True
python ffjord_sgm.py --data 2spirals --depth 7 --dims 8 --rff True

python ffjord_sgm.py --data 2spirals --depth 1 --dims 4 --rff True
python ffjord_sgm.py --data 2spirals --depth 3 --dims 4 --rff True
python ffjord_sgm.py --data 2spirals --depth 5 --dims 4 --rff True
python ffjord_sgm.py --data 2spirals --depth 7 --dims 4 --rff True




