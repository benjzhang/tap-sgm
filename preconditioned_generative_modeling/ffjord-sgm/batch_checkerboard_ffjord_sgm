python ffjord_sgm.py --data checkerboard --depth 5 --dims 16 --tint 0.006 --finalT 0.3 --dt 0.0003 --rff True
python ffjord_sgm.py --data checkerboard --depth 7 --dims 16 --tint 0.006 --finalT 0.3 --dt 0.0003 --rff True

python ffjord_sgm.py --data checkerboard --depth 1 --dims 8 --tint 0.006 --finalT 0.3 --dt 0.0003 --rff True
python ffjord_sgm.py --data checkerboard --depth 3 --dims 8 --tint 0.006 --finalT 0.3 --dt 0.0003 --rff True
